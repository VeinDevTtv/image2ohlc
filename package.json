{
  "name": "candles-from-image",
  "version": "1.0.0",
  "description": "Automated pipeline that converts TradingView candlestick chart screenshots into exact OHLC values",
  "main": "dist/index.js",
  "bin": {
    "scan": "./bin/scan.js",
    "preprocess": "./bin/preprocess-cli.js",
    "segmentation": "./bin/segmentation-cli.js",
    "evaluate": "./bin/evaluate-cli.js"
  },
  "scripts": {
    "build": "tsc",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "start": "node dist/index.js",
    "dev": "ts-node src/index.ts",
    "format": "prettier --write src/**/*.ts",
    "format:check": "prettier --check src/**/*.ts",
    "preprocess": "ts-node bin/preprocess-cli.ts",
    "create-test-images": "ts-node bin/preprocess-cli.ts --create-test-images",
    "dev:frontend": "vite",
    "build:frontend": "tsc && vite build",
    "preview:frontend": "vite preview",
    "segmentation:generate": "ts-node bin/segmentation-cli.ts generate-dataset",
    "segmentation:train": "ts-node bin/segmentation-cli.ts train-model",
    "segmentation:infer": "ts-node bin/segmentation-cli.ts infer",
    "segmentation:pipeline": "ts-node bin/segmentation-cli.ts full-pipeline",
    "evaluate": "ts-node bin/evaluate-cli.ts",
    "dev:backend": "ts-node backend/server.ts",
    "start:backend": "node dist/backend/server.js"
  },
  "keywords": [
    "candlestick",
    "ohlc",
    "trading",
    "image-processing",
    "ocr",
    "tradingview"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@types/chart.js": "^2.9.41",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.11.17",
    "@types/react": "^18.2.55",
    "@types/react-dom": "^18.2.19",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "@vitejs/plugin-react": "^4.2.1",
    "autoprefixer": "^10.4.17",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "jest": "^29.7.0",
    "postcss": "^8.4.35",
    "prettier": "^3.2.5",
    "tailwindcss": "^3.4.1",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3",
    "vite": "^5.1.0"
  },
  "dependencies": {
    "@radix-ui/react-dialog": "^1.0.5",
    "@radix-ui/react-dropdown-menu": "^2.0.6",
    "@radix-ui/react-label": "^2.0.2",
    "@radix-ui/react-slot": "^1.0.2",
    "@radix-ui/react-toast": "^1.1.5",
    "@tensorflow/tfjs-node": "^4.15.0",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/sharp": "^0.31.1",
    "@types/tesseract.js": "^0.0.2",
    "@types/uuid": "^9.0.8",
    "chart.js": "^4.5.1",
    "chartjs-node-canvas": "^5.0.0",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.0",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "html-pdf-node": "^1.0.8",
    "jspdf": "^3.0.3",
    "lucide-react": "^0.323.0",
    "onnxruntime-node": "^1.16.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "sharp": "^0.34.4",
    "tailwind-merge": "^2.2.1",
    "tesseract.js": "^6.0.1",
    "tsconfig-paths": "^4.2.0",
    "uuid": "^9.0.1"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
